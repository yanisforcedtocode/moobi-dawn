      <a href = "/pages/about-us">
        <div class="repairNavi__section__container__options__grids__item">
          <img src="https://cdn.shopify.com/s/files/1/0046/0990/0618/files/carbody.png?v=1635331608" alt="" class="repairNavi__section__container__options__grids__item__img">
