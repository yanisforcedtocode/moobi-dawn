      <div class="featuredCollection__section__h2container">
        <h2>我們的品牌.</h2>
      </div>
      {% for collectionHandle in brandCollectionHandles %}
      <div class="featuredCollection__section__container__item">
        <a href="{{ collections[collectionHandle].url }}">
          <div>
            <img src="{{ collections[collectionHandle].image | image_url: width: 200 }}" width = 200 height = 200 loading = "lazy"/>
          </div>
        </a>
        <p>{{ collections[collectionHandle].title }}</p>
      </div>
      {% endfor %}
