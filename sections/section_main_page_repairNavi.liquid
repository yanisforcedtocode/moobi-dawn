<div class="spacer__section spacer--yellow"></div>
<div class="featuredArticle__section">
  <div class="featuredArticle__section__container">
    <div class="featuredArticle__section__container__h2container">
      <h2>MOOBI 豆知識.</h2>
    </div>
    <div class="featuredArticle__section__container__item" data-naviPage_engagement_type = 'naviPage_engagement_blog'>
      <h3 class="featuredArticle__section__container__item__title">最新連載</h3>
      
      {% assign beautyBlog = blogs["wisdom-car-beauty"].articles%}
      {% assign tutorialBlog = blogs["car-repairs-tutorial"].articles%}
      {% assign repairBlog = blogs["wisdom-repairs"].articles%}
      {% assign startBlog = blogs["wisdom-startup"].articles%}
      {% assign allBlogs = beautyBlog | concat: tutorialBlog | concat: tutorialBlog | concat: repairBlog | concat: startBlog | sort: "created_at" | reverse%}
      {% for article in allBlogs limit:1%}
        <div class="featuredArticle__section__container__item__article">
          <a href="{{ article.url }}"
            ><img data-src="{{ article.image | img_url: '400x' }}" class = "lazyload" />
            <h3>{{ article.title }}</h3></a
          >
          <span>NEW</span>
          <span>{{ article.author }}</span>
          <span>{{ article.published_at | date: "%b %d, %y" }}</span>
          <div style="height: 15px;"></div>
          <p>{{ article.excerpt | strip_html }}</p>
        </div>
      {% endfor %}

      {% for article in blogs["wisdom-car-beauty"].articles limit:1%}
      {% endfor %}
    </div>
    <div class="featuredArticle__section__container__item">
      <h3 class="featuredArticle__section__container__item__title">熱門豆知識</h3>
      <div class="featuredArticle__section__container__item__article">
        {% render 'typesenseClient--article' attribute: 'data-naviPage_engagement_type' attributeValue: 'naviPage_engagement_blog_widget'%}
    </div>
    </div>

    

  
  </div>
</div>

